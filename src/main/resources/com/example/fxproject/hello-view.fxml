<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="468.0" prefWidth="339.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.fxproject.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label prefHeight="18.0" prefWidth="102.0" text="Menu Principal" />

  <Label fx:id="welcomeText" />
   <VBox prefHeight="460.0" prefWidth="400" spacing="10">
      <children>
         <Button onAction="#navigateToAddEvent" prefHeight="26.0" prefWidth="203.0" text="Ajouter événment" />
         <Button onAction="#navigateToUpdateEvent" prefHeight="26.0" prefWidth="201.0" text="Mettre àjour un événment" />
         <Button onAction="#navigateToDeleteEvent" prefHeight="26.0" prefWidth="201.0" text="Supprimer un événment" />
         <Button onAction="#navigateToAddParticipation" prefHeight="26.0" prefWidth="202.0" text="Ajouter une Participation" />
         <Button onAction="#navigateToUpdateParticipation" prefHeight="26.0" prefWidth="202.0" text="Mettre à jour une participation" />
         <Button onAction="#navigateToDeleteParticipation" prefHeight="26.0" prefWidth="204.0" text="Supprimer une participation" />
         <Button onAction="#navigateToConsulterListeevent" prefHeight="26.0" prefWidth="204.0" text="Consulter la liste des événments" />
         <Button onAction="#navigateToConsulterListepart" prefHeight="26.0" prefWidth="205.0" text="Consulter la liste des participations" />
      </children>
   </VBox>
</VBox>
